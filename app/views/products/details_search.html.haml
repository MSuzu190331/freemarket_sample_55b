= render 'index-header-list'
.search-main.clearfix
  .search-main__content
    %section.search-main__content--box
      %h2.search-result
        検索商品
      .search-count
        3件
      .search-item
        %section.search-item__box
          = link_to "showページのパス", class:"search-item__box--show" do
            %figure.item-photo
              = image_tag "data:image/jpeg;base64,", class:"item-photo__img"
            .item-text
              .item-text__name
                にゃん
              .item-text__num
                .item-text__num--price
                  ¥1000
                .item-text__num--icon
                  %i.far.fa-heart
                  %span.good
                    2
  .search-main__side
    = form_with url:details_search_products_path, class:"search-main__side--form" do |f|
      .search-sort
        .search-sort__form
          .search-sort__form--wrap
            %i.fas.fa-angle-down.font-down
            %select.search-sort__form--wrap--select#select-sort
              %option 並び替え
      .search-box
        %h3.search-box__head
          詳細検索
        .form-group
          %lavel.form-group__box
            %i.fas.fa-plus.font-icon
            %span.form-group__box--name
              キーワードを追加する
          = f.text_field:name, placeholder:"例）値下げ", class:"form-group__keyword"
        .form-group
          %lavel.form-group__box
            %i.fas.fa-list-ul.font-icon
            %span.form-group__box--name
              カテゴリーを選択する
          .form-group__wrap
            %i.fas.fa-angle-down
            %select.form-group__wrap--def
              %option
                すべて
        .form-group
          %lavel.form-group__box
            %i.fas.fa-coins.font-icon
            %span.form-group__box--name
              価格
          .form-group__wrap
            %i.fas.fa-angle-down
            %select.form-group__wrap--def
              %option
                選択してください
            = f.text_field:price, placeholder:"￥Min", class:"form-group__wrap--price"
            %span ~
            = f.text_field:price, placeholder:"￥Max", class:"form-group__wrap--price"
        .form-group.clearfix
          %lavel.form-group__box
            %i.fas.fa-star.font-icon
            %span.form-group__box--name
              商品の状態
          %div.form-group__check
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__all"
              %lavel.checkbox__lavel
                すべて
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__one"
              %lavel.checkbox__lavel
                新品、未使用
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__two"
              %lavel.checkbox__lavel
                未使用に近い
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__three"
              %lavel.checkbox__lavel
                目立った傷や汚れなし
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__four"
              %lavel.checkbox__lavel
                やや傷や汚れあり
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__five"
              %lavel.checkbox__lavel
                傷や汚れあり
            .checkbox
            = f.check_box:status, class:"checkbox__def", id:"checkbox__six"
            %lavel.checkbox__lavel
              全体的に状態が悪い
        .form-group.clearfix
          %lavel.form-group__box
            %i.fas.fa-truck-moving.font-icon
            %span.form-group__box--name
              配送料の負担
          %div.form-group__check
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__all"
              %lavel.checkbox__lavel
                すべて
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__one"
              %lavel.checkbox__lavel
                着払い(購入者負担)
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__two"
              %lavel.checkbox__lavel
                送料込み(出品者負担)
        .form-group.clearfix
          %lavel.form-group__box
            %i.fas.fa-shopping-cart.font-icon
            %span.form-group__box--name
              販売状況
          %div.form-group__check
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__all"
              %lavel.checkbox__lavel
                すべて
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__one"
              %lavel.checkbox__lavel
                販売中
            .checkbox
              = f.check_box:status, class:"checkbox__def", id:"checkbox__two"
              %lavel.checkbox__lavel
                売り切れ
        .form-btn.clearfix
          .form-btn__gray クリア
          = f.submit "完了", class:"form-btn__red"
= render "index-footer"
